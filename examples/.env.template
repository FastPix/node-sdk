# @fastpix/fastpix-node SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: FASTPIX_
#
# Security environment variables:
FASTPIX_USERNAME=your-access-token
FASTPIX_PASSWORD=secret-key

# Debug Configuration
# Enable HTTP request/response logging for debugging
FASTPIX_DEBUG=true
